	<parameter name="ALUNO" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT questionario.pergunta,
	questionario.tipoquestionario,
	questionario.aluno_id,
	alunos.nome,
	questionario.curso_id,
	curso.nome,
	questionario.professor_id,
	professor.id,
	pessoa.nome
FROM questionario
	INNER JOIN alunos ON 
	 questionario.aluno_id = alunos.id 
	INNER JOIN curso ON 
	 questionario.curso_id = curso.id 
	INNER JOIN professor ON 
	 questionario.professor_id = professor.id 
	INNER JOIN pessoa ON 
	 professor.id = pessoa.id
	 WHERE aluno_id = $P{ALUNO}]]>
